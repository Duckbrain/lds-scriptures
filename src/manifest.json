{
  "name": "__MSG_app_title__ DEV",
  "description": "__MSG_app_description__",
  "version": "3.0.0",
  "app": 
  {
    "launch": 
    {
      "local_path": "index.html"
    }
  },
  "omnibox": { "keyword" : "lds" },
  "icons":
  {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "content_scripts":
  [
    {
      "matches": ["http://*.lds.org/*", "https://*.lds.org/*"],
      "css": ["css/ldsorg.css"],
      "js": ["js/ldsorg.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "web_accessible_resources": [ "img/icon_16.png" ],
  "permissions": [ "storage", "chrome://favicon/", "bookmarks", "http://*.lds.org/", "https://*.lds.org/" ],
  "options_page": "options.html",
  "default_locale": "en",
  "manifest_version": 2
}
